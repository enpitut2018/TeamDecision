<script>
document.getElementById("subtitle").innerHTML = "グループ分け結果"
</script>
<table>
  <tr>
    <th>Team id</th>
    <th>User id</th>
    <th>Name</th>
    <th>Email</th>
  </tr>

  <% l=[]
  User.where(Rid: session[:u_rid]).each do |u|
    index = l.find_index(u.Tid)
    if index ==nil then
      index=l.length
      l.append(u.Tid)
    end %>
    <tr>
      <td><%= index+1 %></td>
      <td><%= u.id %></td>
      <td><%= u.name %></td>
      <td><%= u.email %></td>
    </tr>
  <% end %>
</table>
